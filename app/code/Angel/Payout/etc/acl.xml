<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Angel_Payout::Payout" sortOrder="10" title="Payout">
					<resource id="Angel_Payout::Payout_save" sortOrder="10" title="Save Payout"/>
					<resource id="Angel_Payout::Payout_delete" sortOrder="20" title="Delete Payout"/>
					<resource id="Angel_Payout::Payout_update" sortOrder="30" title="Update Payout"/>
					<resource id="Angel_Payout::Payout_view" sortOrder="40" title="View Payout"/>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Angel_Payout::config_angel_payout" title="payout"/>
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
